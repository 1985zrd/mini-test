/* components/test/test.wxss */
.test-module {
  display: block;
  width: 100%;
  height: 200rpx;
  line-height: 200rpx;
  border: 1px solid #eee;
}
.dis {
  transition: all 2s;
  transform: translateX(-800rpx);
  /* animation: mymove 2s; */
  border-color: red;
}
@keyframes mymove {
  20% {
    position: fixed;
    top: 0;
    left: 0;
  }
  80% {
    transition: all 2s linear 0 1;
    transform: translateX(-750rpx);
  }
  100% {
    position: fixed;
    top: 0;
    left: -750rpx;
  }
}
